{
  "config": {
    "optimize-autoloader": true,
    "classmap-authoritative": true
  },
  "require": {
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mysqli": "*",
    "monolog/monolog": "*",
    "jabarihunt/json-web-token": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "*"
  },
  "autoload": {
    "psr-4": {
      "Manevia\\": ["classes/Manevia/"],
      "Manevia\\Controllers\\": ["controllers/v1/"]
    }
  }
}